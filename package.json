{
    "name": "wcs-calendar",
    "version": "1.0.0",
    "description": "create calendar for christmas",
    "main": "index.js",
    "scripts": {
        "test": "This is a calendar",
        "build-scss": "node-sass --include scss assets/scss/main.scss assets/css/style.css",
        "build-snow": "node-sass --include scss assets/scss/snow.scss assets/css/snow.css",
        "watch": "nodemon -e scss -x \"npm run build-scss && npm run build-snow\""
    },
